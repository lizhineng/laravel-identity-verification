{
    "name": "lizhineng/laravel-identity-verification",
    "description": "Identity verification with Aliyun for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Li Zhineng",
            "email": "im@zhineng.li"
        }
    ],
    "autoload": {
